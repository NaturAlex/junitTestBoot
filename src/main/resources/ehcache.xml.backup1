<?xml version="1.0" encoding="UTF-8" ?>
<config
        xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
        xmlns='http://www.ehcache.org/v3' xmlns:eh="http://www.ehcache.org/v3"
        xsi:schemaLocation="http://www.ehcache.org/v3
http://www.ehcache.org/schema/ehcache-core.xsd
">
    <eh:persistence directory="./rabbit_ehCache"/>
    <cache alias="rabbitSender">
        <key-type>java.lang.String</key-type>
        <value-type>com.i61.rabbitmq.config.CustomCorrelationData</value-type>
        <resources>
            <disk persistent="true" unit="MB">100</disk> <!--持久化 -->
        </resources>
    </cache>

</config>